


@font-face {
    font-family: "PetitFormalScript";
    src: url(../fonts/PetitFormalScript-Regular.ttf);
}


@font-face {
    font-family: "Comfortaa";
    src: url(../fonts/Comfortaa-Regular.ttf);
    font-weight: 400;
}


@font-face {
    font-family: "Comfortaa";
    src: url(../fonts/Comfortaa-Light.ttf);
    font-weight: 300;
}


@font-face {
    font-family: "Comfortaa";
    src: url(../fonts/Comfortaa-Bold.ttf);
    font-weight: 700;
}

body {
    font-family: Comfortaa;
    background-image:url(../img/6-zen-garden-dirk-ercken.png);
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center bottom;
}
.content{
    background-repeat:no-repeat;
    background-size:cover;
    background-position:center;
    padding-bottom: 0px;
    padding-top: 0px;
    text-align:center;
    color: #405559;
}

.ui-header, .ui-footer {
    opacity: 0.7;
    background-color: white !important;
    border: none;
    height: 65px;
}

.ui-page {
    background: none;
}

.ui-page-theme-a .ui-btn, html .ui-bar-a .ui-btn, html .ui-body-a .ui-btn, html body .ui-group-theme-a .ui-btn, html head+body .ui-btn.ui-btn-a, .ui-page-theme-a .ui-btn:visited, html .ui-bar-a .ui-btn:visited, html .ui-body-a .ui-btn:visited, html body .ui-group-theme-a .ui-btn:visited, html head+body .ui-btn.ui-btn-a:visited{
    color: orange;
    text-shadow:none;
    border: none;
    background: none;
}
.ui-page-theme-a .ui-btn:hover {
    background: none;
}
.face{
    width: 300px;
    height: 300px;
    border: 8px solid #405559;
    border-radius: 50%;
    margin: 70px auto;
}

.flexslider {
    background: none;
    border: none;
    -webkit-box-shadow: none;
    box-shadow: none;
}

.btn-start {
    -webkit-transition: all 1s; /* For Safari 3.1 to 6.0 */
    transition: all 1s;
    opacity: 0;
    width:50%;
    display:block;
    text-align:center;
    background-color:orange !important;
    padding:10px;
    text-decoration:none;
    font-weight:200;
    font-size: 24px;
    height: 20px;
    color:white !important;
    text-shadow: none;
    margin: 15px auto;
}

.center {
    text-align: center;
}

.holder {
    margin: 0 auto;
    cursor: pointer;
}


tspan {
    pointer-events: none;
}

.btn-time-update, .btn-reset, .btn-stop {
    display:inline-block;
    border-radius:50%;
    background-color:#405559;
    width:30px;
    height:30px;
    color:white;
    text-shadow:none;
    vertical-align:bottom;
    margin-right:3px;
    margin-left:3px;
    cursor: pointer;
}
.btn-time-update:active, .btn-reset:active, .btn-stop:active {
    background-color: orange;
}
h5{
	margin-top:18px;
}
.btn-day {
    display:inline-block;
    border:1px solid #405559;
    color: #405559;
    width:30px;
    height:30px;
    text-shadow:none;
    line-height: 6px;
    margin-right:3px;
    margin-left:3px;
    cursor: pointer;
}

.btn-day-selected {
    display:inline-block;
    background-color:#405559;
    color:white;
    text-shadow:none;
    vertical-align:bottom;
    margin-right:3px;
    margin-left:3px;
    cursor: pointer;
}

.xdsoft_datetimepicker .xdsoft_calendar td.xdsoft_default, .xdsoft_datetimepicker .xdsoft_calendar td.xdsoft_current, .xdsoft_datetimepicker .xdsoft_timepicker .xdsoft_time_box >div >div.xdsoft_current {
    background: #405559 !important;
    box-shadow: #334447 0px 1px 3px 0px inset !important;
    text-shadow: none;
    color: #fff;
    font-weight: 700;
}

.xdsoft_datetimepicker .xdsoft_calendar td.xdsoft_today {
    color: orange !important;
}

.btn-dialog {
    background-color: orange !important;
    color: white !important;
    text-shadow: none !important;
}
a {
    text-shadow: none !important;
}
.clock_container{
    position: fixed;
    top: 70px;
    left: 0;
    right:0;
    z-index:2;
}

.extra_container{
    margin-top:10px;
    text-align:center;
}

.ui-btn {
   box-shadow: none;
}

.face{
    margin-top:30px;
    margin-left:20px;
    width:64px;
    height:64px;
    border-radius:50%;
    display:inline-block;
    cursor: pointer;
}
.left-eye{
    width:1px;
    height:1px;
    border-radius:50%;
    background-color:#ef2929;
    margin-top:14px;
    margin-left:3px;
    display:inline-block;
}
.left-eye.satisfied{
    background-color:#73d216;
}
.left-eye.ok{
    background-color:#729fcf;
}
.right-eye.ok{
    background-color:#729fcf;
}
.right-eye{
    display:inline-block;
    width: 1px;
    height: 1px;
    border-radius:50%;
    background-color:#ef2929;
    margin-top:14px;
    margin-left:10px;
}
.right-eye.satisfied{
    background-color:#73d216;
}
.satisfied{
    color:#73d216;
    border:8px solid #73d216;}
.unsatisfied{
    color:#ef2929;
    border:8px solid #ef2929;}
.ok{
    color:#729fcf;
    border:8px solid #729fcf;}
.mouth{
    width:28px;
    height:2px;
    border:4px solid #729fcf;
    background-color:#729fcf;
    margin-left:14px;
    margin-top:5px;
}
.sad-mouth{
    height:10px;
    width:20px;
    border-radius:90px 90px 0 0;
    margin:0px auto 0;
    border:8px solid #ef2929;
    border-bottom:none;
    position:relative;
    background:transparent;
}
.smiley-mouth{
    height:10px;
    width:20px;
    border-radius: 0 0 90px 90px;
    margin:4px auto 0;
    border:8px solid #73d216;
    border-top:none;
    position:relative;
    background:transparent;
}

.logo {
    position: absolute;
    top: 10px;
    left: 60px;
}

.page-title {
    color: #181818 !important;
    font-size: 20px !important;
}

.quote {
    font-family: PetitFormalScript;
}

li.ui-happy:before{
    content: url('../img/smiley-happy.png');
    margin: 0 5px;
}

.ui-happy {
    color: #73d216 !important;
}

li.ui-neutral:before{
    content: url('../img/smiley-neutral.png');
    margin: 0 5px;
}

.ui-neutral {
    color: #729fcf !important;
}

li.ui-sad:before{
    content: url('../img/smiley-sad.png');
    margin: 0 5px;
}

.ui-sad {
    color: #ef2929 !important;
    margin: 0 5px;
}

.alarm-time {
    font-size: 32px;
}

.alarm-inactive {
    color: grey !important;
}

.alarm-active {
    color: orange !important;
}
#starting-timer{
	overflow-y:hidden;
	}
#quoteContainer{
	display:none;
	text-align:center;
	margin-top:50px;}
#quoteContainer #quote{
	padding:20px;
}
#quoteContainer #author{
	text-align: right;margin-right:20px;
}

.ui-mobile .ui-page-active{
	overflow-y:hidden}
.ui-content{
	padding:0px;
}
.ui-header .ui-title, .ui-footer .ui-title{
	margin:0 10%;
	padding:1em 0;
}

